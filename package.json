    "sinon-chai": "^2.14.0",
    "url-loader": "^0.5.9",
    "validate-commit-msg": "^2.14.0",
    "vue-loader": "^13.7.1",
    "vue-style-loader": "^3.1.2",
    "vue-test-utils": "^0.5.0",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.2"
  },
  "config": {
    "deployToGit": {
      "repository": "https://$GH_TOKEN@github.com/shierro/cv.git",
      "branch": "gh-pages",
      "folder": "docs",
      "script": "npm run build",
      "commit": "$npm_package_version",
      "user": {
        "email": "theodoro.pelingan@gmail.com",
        "name": "Theo Jenkins"
      }
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
